body
 <!-- Main Div -->
 div(class="site-wrapper")
   <!--left Sidebar-->
   nav(class="left-side-nav")
    h1 MEME CREATOR
    ul
     li 
      a 
       i(class="fa fa-star") 
       |Welcome
     li 
      a 
       i(class="fa fa-home") 
       |Home
     li 
      a 
       i(class="fa fa-close") 
       |Close
     li 
       a 
        i(class="fa fa-upload") 
        form(action="/uploadPic",method="POST",enctype="multipart/form-data",id="upload-form",style="display:inline-block;padding:0;margin:0")
         input(type='hidden',value=mainImage,id="uploadImage")
         label(for="file-upload",class="file-upload-wrapper")
          |File
         input(type="file",name="file",id="file-upload")
         &nbsp;
      
     li 
      a 
       input(id="colorSelector",class="side-color colorSelector",style="")
       &nbsp;Select Color 
     li 
      a 
       i(class="fa fa-file-photo-o") 
       label(for="files",class="file-upload-wrapper")
        |Upload Smily
       input(type="file",id="files")
    

   <!-- Right Nav Bar -->
   nav(class="right-side-nav")
   <!--mainpage-->
   main
    <!--Main Heading -->
    header
      <!-- left menu Icon -->
      div(class="menuBar")
       <i class="fa fa-3x fa-list"></i>
      <!-- Main Heading -->
      div(class="heading")
       h1 MEME CREATOR
      <!--right menu -->
      div(class="menubar-right") 
    <!--Main Page -->
    div(style="",id="main-page-wrapper")
     div(style="",id="drawing-wrapper")
       div(style="position:relative;border: 4px double rgb(219, 219, 219);float: left;box-shadow: 1px 2px 10px rgb(140, 136, 136);box-sizing:border-box;margin-bottom:15px",width="40%",height="480px",id="drawing-canvas") 
